
public class Morpion {
	//ATTRIBUTES
	private int[][] tab;
	
	
	//METHODS
	public Morpion(){
		this.tab = new int[3][3];
		for(int i = 0; i < 3; i ++){
			for(int j = 0; j < 3; j ++){
				int[i][j] = 0;
			}
		}
	}
	
	public Morpion(int[][] t){
		this.tab = t;
	}
	
	public int[][] getTab(){
		return this.tab;
	}
	
}
